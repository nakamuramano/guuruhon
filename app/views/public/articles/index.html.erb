  <div class="container">
    <div class="row">
      <div class='col-md-3'>
        <%# サイドブロック ユーザープロフィール %>
        <%= render 'public/users/list', bookmark: @bookmarks, user: @user %>
        <%# サイドブロック タグ %>
        <%= render 'tag', tag: @tags %>
      </div>
      <%# メインブロック 投稿一覧 %>
      <%= render 'articles', article: @articles %>
    </div>
    <div class="pagination"><%= paginate @articles %></div>
  </div>
